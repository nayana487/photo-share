<h1>Your Albums <%= link_to "(+)", new_album_path %> </h1>

<% @albums.each do |album| %>

  <div class="albums">
    <h2> <%= link_to album.title, album_path(album) %> </h2>
         <%= link_to image_tag(album.img_url), album_path(album) %>
          <br>
          <time> Created at <%= album.created_at %></time>

  </div>
    <% end %>
