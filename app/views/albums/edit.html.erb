<div class="well">

<h1>Edit Album</h1>

<%= form_for @album do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit "Update" %>
<% end %>
</div>

<h2> <%= link_to "Delete Album", @album, method: :delete %> </h2>
<h2> <%= link_to "< back to Albums", albums_path(@album) %> </h2>
